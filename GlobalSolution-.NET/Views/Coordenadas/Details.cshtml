@model GlobalSolution_.NET.Models.CoordenadasModel

@{
    ViewData["Title"] = "Details Coordenadas";
}

<h1>@ViewData["Title"]</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.longitude)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.longitude)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.latitude)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.latitude)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id_coordenadas="@Model?.id_coordenadas">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
